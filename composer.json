{
    "name": "quartzy/php-email",
    "type": "library",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "description": "A domain model for describing emails that is indifferent towards sending methods.",
    "keywords": ["email", "domain-driven"],
    "authors": [
        {
            "name": "Chris Muthig",
            "email": "camuthig@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "beberlei/assert": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Postcard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Postcard\\Test\\": "tests/"
        }
    }
}
